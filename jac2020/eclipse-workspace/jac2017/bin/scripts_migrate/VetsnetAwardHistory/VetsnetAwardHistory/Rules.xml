<decisionRules>

  <decision type='CP_ALLOTDEC'>
    <rules>
      <rule>

        <condition sequence='1' when='Use (a) when the Allotment Amount is not blank.'>
          <field test='notBlank'>ALTMNT_AMT</field>
          <phrase text='On '/>
          <phrase field='DECN_DT'/>
          <phrase text=', the Allotment Decision was '/>
          <phrase field='ALTMNT_DECN_TN' subfield='ALTMNT_DECN_TC'/>
          <phrase text=' in the amount of '/>
          <phrase field='ALTMNT_AMT' format='currency'/>
          <phrase text=' monthly to '/>
          <phrase field='PTCPNT_ID' participant='true'/>
          <phrase text=' effective '/>
          <phrase field='AWD_EFCTV_DT'/>
          <phrase text='.'/>
        </condition>

        <condition sequence='2' when='Use (c) when the Allotment Amount is blank.'>
          <field test='isBlank'>ALTMNT_AMT</field>
          <phrase text='On '/>
          <phrase field='DECN_DT'/>
          <phrase text=', the Allotment Decision for '/>
          <phrase field='PTCPNT_ID' participant='true'/>
          <phrase text=' was '/>
          <phrase field='ALTMNT_DECN_TN' subfield='ALTMNT_DECN_TC'/>
          <phrase text='.'/>
        </condition>

        <condition sequence='3' when='Use (b) when the Invalid Decision Date is not blank.'>
          <appendsTo>1</appendsTo>
          <appendsTo>2</appendsTo>
          <field test='notBlank'>INVLD_DECN_DT</field>
          <phrase text=' This decision was invalidated on '/>
          <phrase field='INVLD_DECN_DT'/>
          <phrase text='.'/>
        </condition>

      </rule>
    </rules>
  </decision>

  <decision type='CP_BASELIGDEC'>
    <rules>
      <rule>

        <condition sequence='1' when='Use (a) when the Award Effective Date is not blank.'>
          <field test='notBlank'>AWD_EFCTV_DT</field>
          <phrase text='On '/>
          <phrase field='DECN_DT'/>
          <phrase text=', the decision was '/>
          <phrase field='DECN_TN' subfield='DECN_TC'/>
          <phrase text=' effective '/>
          <phrase field='AWD_EFCTV_DT'/>
          <phrase text='.'/>
        </condition>

        <condition sequence='2' when='Use (g) when Pay Grade is present.'>
          <appendsTo>1</appendsTo>
          <field test='notBlank' connector='and'>AWD_EFCTV_DT</field>
          <field test='notBlank'>PAY_GRADE_TN</field>
          <phrase text=" The veteran's Pay Grade was recorded as "/>
          <phrase field='PAY_GRADE_TN' subfield='PAY_GRADE_TC'/>
          <phrase text='.'/>
        </condition>

        <condition sequence='3' when='Use (b) when the Award Effective Date is blank.'>
          <field test='isBlank'>AWD_EFCTV_DT</field>
          <phrase text='On '/>
          <phrase field='DECN_DT'/>
          <phrase text=', the decision was '/>
          <phrase field='DECN_TN' subfield='DECN_TC'/>
          <phrase text='.'/>
        </condition>

        <condition sequence='4' when='Use (c) when the Invalid Decision Date is not blank.'>
          <appendsTo>1</appendsTo>
          <appendsTo>3</appendsTo>
          <field test='notBlank'>INVLD_DECN_DT</field>
          <phrase text=' The decision was invalidated on '/>
          <phrase field='INVLD_DECN_DT'/>
          <phrase text='.'/>
        </condition>

      </rule>
    </rules>
  </decision>

  <decision type='CP_BRLBFT_DECN'>
    <rules>
      <rule>

        <condition sequence='1' when='Use (a) when a decision record exists.'>
          <phrase text='On '/>
          <phrase field='DECN_DT'/>
          <phrase text=', the '/>
          <phrase field='BURIAL_BNFT_TN' subfield='BURIAL_BNFT_TC'/>
          <phrase text=' Decision was '/>
          <phrase field='BURIAL_DECN_TN' subfield='BURIAL_DECN_TC'/>
          <phrase text='.'/>
        </condition>

        <condition sequence='2' when='Use (b) when the Invalid Decision Date is not blank.'>
          <appendsTo>1</appendsTo>
          <field test='notBlank'>INVLD_DECN_DT</field>
          <phrase text=' This decision was invalidated on '/>
          <phrase field='INVLD_DECN_DT'/>
          <phrase text='.'/>
        </condition>

     </rule>
    </rules>
  </decision>

  <decision type='CP_BRLBEN_DECN'>
    <rules>
      <rule>

        <condition sequence='1' when='Use (a) when a decision record exists.'>
          <phrase text='On '/>
          <phrase field='DECN_DT'/>
          <phrase text=', the Decision for '/>
          <phrase field='PTCPNT_ID_C' participant='true'/>
          <phrase text=' was '/>
          <phrase field='BENE_DECN_TN' subfield='BENE_DECN_TC'/>
          <phrase text='.'/>
        </condition>

        <condition sequence='2' when='Use (b) when the Invalid Decision Date is not blank.'>
          <appendsTo>1</appendsTo>
          <field test='notBlank'>INVLD_DECN_DT</field>
          <phrase text=' This decision was invalidated on '/>
          <phrase field='INVLD_DECN_DT'/>
          <phrase text='.'/>
        </condition>

     </rule>
    </rules>
  </decision>

  <decision type='CP_BRLSUM_DECN'>
    <rules>
      <rule>

        <condition sequence='1' when='Use (a) when a decision record exists.'>
          <phrase text='For the Burial Decision recorded on '/>
          <phrase field='DECN_DT'/>
          <phrase text=', the Final Disposition Date was '/>
          <phrase field='FINAL_DISPTN_DT'/>
          <phrase text=', the Total Burial Expenses were '/>
          <phrase field='TL_BURIAL_EXPNS'/>
          <phrase text=', the Transportation expenses were '/>
          <phrase field='TRNSPN_EXPNSS'/>
          <phrase text=' and the Marker Purchase Date was '/>
          <phrase field='MARKER_PRCHS_DT'/>
          <phrase text='.'/>
        </condition>

        <condition sequence='2' when='Use (b) when the Invalid Decision Date is not blank.'>
          <appendsTo>1</appendsTo>
          <field test='notBlank'>INVLD_DECN_DT</field>
          <phrase text=' This decision was invalidated on '/>
          <phrase field='INVLD_DECN_DT'/>
          <phrase text='.'/>
        </condition>

      </rule>
    </rules>
  </decision>

  <decision type='CP_CUSTCHLD_DECN'>
    <rules>
      <rule>

        <condition sequence='1' when='Use (a) when a decision record exists.'>
          <phrase text='On '/>
          <phrase field='DECN_DT'/>
          <phrase text=', '/>
          <phrase field='PTCPNT_ID_C' participant='true'/>
          <phrase text=' was recorded as being in the legal custody of '/>
          <phrase field='CUSTDY_TN' subfield='CUSTDY_TC'/>
          <phrase text=' on '/>
          <phrase field='CUSTDY_DT'/>
          <phrase text='.'/>
        </condition>

        <condition sequence='2' when='Use (f) when there is a value in Affects Childs Award On Date'>
          <appendsTo>1</appendsTo>
          <field test='notBlank'>AFFCTCHLD_DT</field>
          <phrase text=" If an adjustment was required to the Child's award, it was to be applied on "/>
          <phrase field='AFFCTCHLD_DT'/>
          <phrase text='.'/>
        </condition>

        <condition sequence='3' when='Use (b) when the Invalid Decision Date is not blank.'>
          <appendsTo>1</appendsTo>
          <field test='notBlank'>INVLD_DECN_DT</field>
          <phrase text=' This decision was invalidated on '/>
          <phrase field='INVLD_DECN_DT'/>
          <phrase text='.'/>
        </condition>

      </rule>
    </rules>
  </decision>

  <decision type='CP_CLOTH_DECN'>
    <rules>
      <rule>

        <condition sequence='1' when='Use (a) when a decision record exists.'>
          <phrase text='On '/>
          <phrase field='DECN_DT'/>
          <phrase text=', the Decision on Clothing Allowance was '/>
          <phrase field='DECN_TN' subfield='DECN_TC'/>
          <phrase text=' for Eligibility Year '/>
          <phrase field='ELGBTY_YR'/>
          <phrase text='.'/>
        </condition>

        <condition sequence='2' when='Use (b) when the Invalid Decision Date is not blank.'>
          <appendsTo>1</appendsTo>
          <field test='notBlank'>INVLD_DECN_DT</field>
          <phrase text=' This decision was invalidated on '/>
          <phrase field='INVLD_DECN_DT'/>
          <phrase text='.'/>
        </condition>

      </rule>
    </rules>
  </decision>

  <decision type='CP_DEPENDDEC'>
    <rules>
      <rule>

        <condition sequence='1' when='Use (a) when the Award Effective Date is not blank.'>
          <field test='compareNotEquals' value='CPDC' connector='and'>AWD_TC</field>
          <field test='notBlank'>AWD_EFCTV_DT</field>
          <phrase text='On '/>
          <phrase field='DECN_DT'/>
          <phrase text=', the decision '/>
          <phrase field='DEPNCY_DECN_TN' subfield='DEPNCY_DECN_TC'/>
          <phrase text=' was recorded for '/>
          <phrase field='PER_PTCPNT_ID' participant='true'/>
          <phrase text=', along with the award status of '/>
          <phrase field='DEPNCY_STT_TN' subfield='DEPNCY_STT_TC'/>
          <phrase text=' and an award effective date of '/>
          <phrase field='AWD_EFCTV_DT'/>
          <phrase text='.'/>
        </condition>

        <condition sequence='2' when='Use (b) when the Award Effective Date is blank.'>
          <field test='compareNotEquals' value='CPDC' connector='and'>AWD_TC</field>
          <field test='isBlank'>AWD_EFCTV_DT</field>
          <phrase text='On '/>
          <phrase field='DECN_DT'/>
          <phrase text=', the decision '/>
          <phrase field='DEPNCY_DECN_TN' subfield='DEPNCY_DECN_TC'/>
          <phrase text=' was recorded for '/>
          <phrase field='PER_PTCPNT_ID' participant='true'/>
          <phrase text=', along with the award status of '/>
          <phrase field='DEPNCY_STT_TN' subfield='DEPNCY_STT_TC'/>
          <phrase text='.'/>
        </condition>

        <condition sequence='3' when='Use (c) when the Invalid Decision Date is not blank.'>
          <appendsTo>1</appendsTo>
          <appendsTo>2</appendsTo>
          <field test='compareNotEquals' value='CPDC' connector='and'>AWD_TC</field>
          <field test='notBlank'>INVLD_DECN_DT</field>
          <phrase text=' This decision was invalidated on '/>
          <phrase field='INVLD_DECN_DT'/>
          <phrase text='.'/>
        </condition>

        <condition sequence='4' when='Use (a) when award type is CPDC.'>
          <field test='compareEquals' value='CPDC'>AWD_TC</field>
          <phrase text='On '/>
          <phrase field='DECN_DT'/>
          <phrase text=', the decision '/>
          <phrase field='DEPNCY_DECN_TN' subfield='DEPNCY_DECN_TC'/>
          <phrase text=' was recorded for '/>
          <phrase field='PER_PTCPNT_ID' participant='true'/>
          <phrase text=', along with the award status '/>
          <phrase field='DEPNCY_STT_TN' subfield='DEPNCY_STT_TC'/>
          <phrase text='.'/>
        </condition>

        <condition sequence='5' when='Use (b) when the award effective date is not blank.'>
          <appendsTo>4</appendsTo>
          <field test='compareEquals' value='CPDC' connector='and'>AWD_TC</field>
          <field test='notBlank'>AWD_EFCTV_DT</field>
          <phrase text=' The award effective date was recorded as '/>
          <phrase field='AWD_EFCTV_DT'/>
          <phrase text='.'/>
        </condition>

        <condition sequence='6' when='Use (c) when the Invalid Decision Date is not blank.'>
          <appendsTo>4</appendsTo>
          <field test='compareEquals' value='CPDC' connector='and'>AWD_TC</field>
          <field test='notBlank'>INVLD_DECN_DT</field>
          <phrase text=' This decision was invalidated on '/>
          <phrase field='INVLD_DECN_DT'/>
          <phrase text='.'/>
        </condition>

      </rule>
    </rules>
  </decision>

  <decision type='CP_DICSP_DECN'>
    <rules>
      <rule>

        <condition sequence='1' when='Use (a) when a decision record exists.'>
          <phrase text='On '/>
          <phrase field='DECN_DT'/>
          <phrase text=", the DIC Spouse's Award Status was recorded as "/>
          <phrase field='DICSP_TN' subfield='DICSP_TC'/>
          <phrase text=' effective '/>
          <phrase field='DICSPAWD_DT'/>
          <phrase text='.'/>
        </condition>

        <condition sequence='2' when='Use (f) when Affected Childs Award Date is not blank.'>
          <appendsTo>1</appendsTo>
          <field test='notBlank'>AFFCTCHLD_DT</field>
          <phrase text=" If an adjustment to the child's award was necessary, the child's award adjustment date was recorded as "/>
          <phrase field='AFFCTCHLD_DT'/>
          <phrase text='.'/>
        </condition>

        <condition sequence='3' when='Use (b) when the Invalid Decision Date is not blank.'>
          <appendsTo>1</appendsTo>
          <field test='notBlank'>INVLD_DECN_DT</field>
          <phrase text=' This decision was invalidated on '/>
          <phrase field='INVLD_DECN_DT'/>
          <phrase text='.'/>
        </condition>

      </rule>
    </rules>
  </decision>

  <decision type='CP_DSBLTYDEC'>
    <rules>
      <rule>
 
        <condition sequence='1' when='Use (a) when a decision record exists.'>
          <phrase text='On '/>
          <phrase field='DECN_DT'/>
          <phrase text=', the Decision was to begin recoupment of '/>
          <phrase field='ORIGNL_BL'/>
          <phrase text=' from the Compensation award effective '/>
          <phrase field='AWD_EFCTV_DT'/>
          <phrase text=' due to receipt of '/>
          <phrase field='DIS_PAY_TN' subfield='DIS_PAY_TC'/>
          <phrase text='.'/>
        </condition>

        <condition sequence='2' when='Use (b) when the Invalid Decision Date is not blank.'>
          <appendsTo>1</appendsTo>
          <field test='notBlank'>INVLD_DECN_DT</field>
          <phrase text=' This decision was invalidated on '/>
          <phrase field='INVLD_DECN_DT'/>
          <phrase text='.'/>
        </condition>

      </rule>
    </rules>
  </decision>

  <decision type='CP_DRILLPAYDEC'>
    <rules>
      <rule>

        <condition sequence='1' when='Use (a) when a decision record exists.'>
          <phrase text='On '/>
          <phrase field='DECN_DT'/>
          <phrase text=', the Decision was to offset '/>
          <phrase field='DYS_NO'/>
          <phrase text=' days of Compensation payments effective '/>
          <phrase field='AWD_EFCTV_DT'/>
          <phrase text=' based on Drill Periods during Fiscal Year '/>
          <phrase field='FISCAL_YEAR_NO'/>
          <phrase text='.'/>
        </condition>

        <condition sequence='2' when='Use (b) when the Invalid Decision Date is not blank.'>
          <appendsTo>1</appendsTo>
          <field test='notBlank'>INVLD_DECN_DT</field>
          <phrase text=' This decision was invalidated on '/>
          <phrase field='INVLD_DECN_DT'/>
          <phrase text='.'/>
        </condition>

      </rule>
    </rules>
  </decision>

  <decision type='CP_AWDELECTDEC'>
    <rules>
      <rule>

        <condition sequence='1' when='Use (a) when the Award Effective Date is not blank.'>
          <field test='notBlank'>AWD_EFCTV_DT</field>
          <phrase text='On '/>
          <phrase field='DECN_DT'/>
          <phrase text=', the Decision on the election of '/>
          <phrase field='ELECTN_TN' subfield='ELECTN_TC'/>
          <phrase text=' was '/>
          <phrase field='ELECTN_DECN_TN' subfield='ELECTN_DECN_TC'/>
          <phrase text=' effective '/>
          <phrase field='AWD_EFCTV_DT'/>
          <phrase text='.'/>
        </condition>

        <condition sequence='2' when='Use (b) when the Award Effective Date is blank.'>
          <field test='isBlank'>AWD_EFCTV_DT</field>
          <phrase text='On '/>
          <phrase field='DECN_DT'/>
          <phrase text=', the Decision on the election of '/>
          <phrase field='ELECTN_TN' subfield='ELECTN_TC'/>
          <phrase text=' was '/>
          <phrase field='ELECTN_DECN_TN' subfield='ELECTN_DECN_TC'/>
          <phrase text='.'/>
        </condition>

        <condition sequence='3' when='Use (c) when the Invalid Decision Date is not blank.'>
          <appendsTo>1</appendsTo>
          <appendsTo>2</appendsTo>
          <field test='notBlank'>INVLD_DECN_DT</field>
          <phrase text=' This decision was invalidated on '/>
          <phrase field='INVLD_DECN_DT'/>
          <phrase text='.'/>
        </condition>

      </rule>
    </rules>
  </decision>

  <decision type='CP_EXPNSDEC'>
    <rules>
      <rule>

        <condition sequence='1' when='Use (a) when From is not blank and To is blank.'>
          <field test='notBlank' connector='and'>FROM_DT</field>
          <field test='isBlank'>TO_DT</field>
          <phrase text='On '/>
          <phrase field='DECN_DT'/>
          <phrase text=', expenses of '/>
          <phrase field='USED_AMT' format='currency'/>
          <phrase text=' with a frequency of '/>
          <phrase field='FRQNCY_TN' subfield='FRQNCY_TC'/>
          <phrase text=' was recorded from '/>
          <phrase field='FROM_DT'/>
          <phrase text=' with an expense type of '/>
          <phrase field='EXPENSE_TN' subfield='EXPENSE_TC'/>
          <phrase text=' were recorded.'/>
        </condition>

        <condition sequence='2' when='Use (b) when From is not blank and To is not blank.'>
          <field test='notBlank' connector='and'>FROM_DT</field>
          <field test='notBlank'>TO_DT</field>
          <phrase text='On '/>
          <phrase field='DECN_DT'/>
          <phrase text=', expenses of '/>
          <phrase field='USED_AMT' format='currency'/>
          <phrase text=' with a frequency of '/>
          <phrase field='FRQNCY_TN' subfield='FRQNCY_TC'/>
          <phrase text=' was recorded from '/>
          <phrase field='FROM_DT'/>
          <phrase text=' to '/>
          <phrase field='TO_DT'/>
          <phrase text=' with an expense type of '/>
          <phrase field='EXPENSE_TN' subfield='EXPENSE_TC'/>
          <phrase text=' were recorded.'/>
        </condition>

        <condition sequence='3' when='Use (c) when the Invalid Decision Date is not blank.'>
          <appendsTo>1</appendsTo>
          <appendsTo>2</appendsTo>
          <field test='notBlank'>INVLD_DECN_DT</field>
          <phrase text=' This decision was invalidated on '/>
          <phrase field='INVLD_DECN_DT'/>
          <phrase text='.'/>
        </condition>

      </rule>
    </rules>
  </decision>

  <decision type='CP_INCOMEDEC'>
    <rules>
      <rule>

        <condition sequence='1' when='Use (a) when award type name is not CPD Parent, From is not blank and To is blank.'>
          <field test='compareNotEquals' value='CPDP' connector='and'>AWD_TC</field>
          <field test='notBlank' connector='and'>FROM_DT</field>
          <field test='isBlank'>TO_DT</field>
          <phrase text='On '/>
          <phrase field='DECN_DT'/>
          <phrase text=' income from '/>
          <phrase field='INCOME_TN' subfield='INCOME_TC'/>
          <phrase text=' of '/>
          <phrase field='USED_AMT' format='currency'/>
          <phrase text=' '/>
          <phrase field='FRQNCY_TN' subfield='FRQNCY_TC'/>
          <phrase text=' from '/>
          <phrase field='FROM_DT'/>
          <phrase text=' was recorded for '/>
          <phrase field='PER_PTCPNT_ID' participant='true'/>
          <phrase text='.'/>
        </condition>

        <condition sequence='2' when='Use (b) when award type name is not CPD Parent, From is not blank and To is not blank.'>
          <field test='compareNotEquals' value='CPDP' connector='and'>AWD_TC</field>
          <field test='notBlank' connector='and'>FROM_DT</field>
          <field test='notBlank'>TO_DT</field>
          <phrase text='On '/>
          <phrase field='DECN_DT'/>
          <phrase text=' income from '/>
          <phrase field='INCOME_TN' subfield='INCOME_TC'/>
          <phrase text=' of '/>
          <phrase field='USED_AMT' format='currency'/>
          <phrase text=' '/>
          <phrase field='FRQNCY_TN' subfield='FRQNCY_TC'/>
          <phrase text=' from '/>
          <phrase field='FROM_DT'/>
          <phrase text=' to '/>
          <phrase field='TO_DT'/>
          <phrase text=' was recorded for '/>
          <phrase field='PER_PTCPNT_ID' participant='true'/>
          <phrase text='.'/>
        </condition>

        <condition sequence='3' when='Use (c) when award type is CPDP.'>
          <field test='compareEquals' value='CPDP'>AWD_TC</field>
          <phrase text='On '/>
          <phrase field='DECN_DT'/>
          <phrase text=' income from '/>
          <phrase field='INCOME_TN' subfield='INCOME_TC'/>
          <phrase text=' of '/>
          <phrase field='CYR_REPRTD_AMT' format='currency'/>
          <phrase text=' annually was recorded for '/>
          <phrase field='FROM_DT'/>
          <phrase text='. The countable portion of this income was '/>
          <phrase field='CYR_CNTBL_AMT' format='currency'/>
          <phrase text='.'/>
        </condition>

        <condition sequence='4' when='Use (h) when exclusion type and exclusion amount contain a value.'>
          <appendsTo>1</appendsTo>
          <appendsTo>2</appendsTo>
          <appendsTo>3</appendsTo>
          <field test='notBlank' connector='and'>EXCLSN_TN</field>
          <field test='notBlank'>EXCLSN_AMT</field>
          <phrase text=' An exclusion amount of '/>
          <phrase field='EXCLSN_AMT' format='currency'/>
          <phrase text=' was deducted due to '/>
          <phrase field='EXCLSN_TN' subfield='EXCLSN_TC'/>
          <phrase text='.'/>
        </condition>

        <condition sequence='5' when='Use (d) when the Invalid Decision Date is not blank.'>
          <appendsTo>1</appendsTo>
          <appendsTo>2</appendsTo>
          <appendsTo>3</appendsTo>
          <field test='notBlank'>INVLD_DECN_DT</field>
          <phrase text=' This decision was invalidated on '/>
          <phrase field='INVLD_DECN_DT'/>
          <phrase text='.'/>
        </condition>

      </rule>
    </rules>
  </decision>

  <decision type='CP_BENEINSTDEC'>
    <rules>
      <rule>

        <condition sequence='1' when='Use (a) when Begin is not blank and End is not blank.'>
          <field test='compareNotEquals' value='CA' connector='and'>AWD_TC</field>
          <field test='notBlank' connector='and'>BEGIN_DT</field>
          <field test='notBlank'>END_DT</field>
          <phrase text='On '/>
          <phrase field='DECN_DT'/>
          <phrase text=', the Decision was to apply the '/>
          <phrase field='INSTZN_TN' subfield='INSTZN_EFCTV_TC'/>
          <phrase text=' from '/>
          <phrase field='BEGIN_DT'/>
          <phrase text=' to '/>
          <phrase field='END_DT'/>
          <phrase text='.'/>
        </condition>

        <condition sequence='2' when='Use (b) when Begin is not blank and End is blank.'>
          <field test='compareNotEquals' value='CA' connector='and'>AWD_TC</field>
          <field test='notBlank' connector='and'>BEGIN_DT</field>
          <field test='isBlank'>END_DT</field>
          <phrase text='On '/>
          <phrase field='DECN_DT'/>
          <phrase text=', the Decision was to apply the '/>
          <phrase field='INSTZN_TN' subfield='INSTZN_EFCTV_TC'/>
          <phrase text=' from '/>
          <phrase field='BEGIN_DT'/>
          <phrase text='.'/>
        </condition>

        <condition sequence='3' when='Use (c) when the Invalid Decision Date is not blank.'>
          <appendsTo>1</appendsTo>
          <appendsTo>2</appendsTo>
          <field test='compareNotEquals' value='CA' connector='and'>AWD_TC</field>
          <field test='notBlank'>INVLD_DECN_DT</field>
          <phrase text=' This decision was invalidated on '/>
          <phrase field='INVLD_DECN_DT'/>
          <phrase text='.'/>
        </condition>

        <condition sequence='4' when='Use (d) when the award type is CA.'>
          <field test='compareEquals' value='CA' connector='and'>AWD_TC</field>
          <field test='isBlank' connector='and'>BEGIN_DT</field>
          <field test='isBlank'>END_DT</field>
          <phrase text='On '/>
          <phrase field='DECN_DT'/>
          <phrase text=', the Decision was to apply the Incarceration Reduction to the Clothing Allowance Payment.'/>
        </condition>

        <condition sequence='5' when='Use (h) and (i) to add related text.'>
          <search>searchObject-&#62;getAWD_DECN_TYPEValue().compareTo("CP_INSTZN") &amp;&amp;</search>
          <search>searchObject-&#62;getINSTZN_SNValue().compareTo(cp_awd_decn->getINSTZN_SNValue()) == 0 &amp;&amp;</search>
          <search>searchObject-&#62;isAWD_DECN_USEDEmpty() == TRUE</search>
        </condition>

        <condition sequence='6' when='Use (c) when the Invalid Decision Date is not blank.'>
